
@model IEnumerable<LaundryOnline.Models.Order>
@{
    Layout = null;
}

<!DOCTYPE html>
<html class="no-js" lang="en">

<head>
    <!-- Meta Tags -->
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="author" content="Laralink">
    <!-- Site Title -->
    <link rel="stylesheet" href="~/pdf/style.css">
</head>

<body>
    <div class="tm_container">
        <div class="tm_invoice_wrap">
            <div class="tm_invoice tm_style1 tm_type3" id="tm_download_section">
                <div class="tm_shape_1">
                    <svg width="1100" height="151" viewBox="0 0 1100 151" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M1100 0.889398H0V150.889H184.505C216.239 150.889 246.673 141.531 269.113 124.872L359.112 58.0565C381.553 41.3977 411.987 32.0391 443.721 32.0391H850V0.889398Z" fill="#007AFF" fill-opacity="0.1" />
                    </svg>
                </div>
                <div class="tm_shape_2">
                    <svg width="1100" height="151" viewBox="0 0 1100 151" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M0 150.889H850V0.889408H665.496C633.762 0.889408 603.327 10.2481 580.887 26.9081L490.888 93.7224C468.447 110.381 438.014 119.74 406.279 119.74H0V150.889Z" fill="#007AFF" fill-opacity="0.1" />
                    </svg>
                </div>
                <div class="tm_invoice_in">
                    <div class="tm_invoice_head tm_align_center tm_mb20">

                        <div class="tm_invoice_left">
                            <div class="tm_logo"><img src="https://dtdrycleaning.wpengine.com/wp-content/themes/dry-cleaning/images/logo.png" alt="Logo" width="250"></div>
                        </div>
                    </div>
                    <div class="tm_invoice_info_list">
                        <br />
                    </div>
                    <
                    <div class="tm_table tm_style1 tm_mb30">
                        <div class="tm_table_responsive">
                            <table class="tm_border_bottom">
                                <thead>
                                    <tr class="tm_border_top">
                                        <th class="tm_width_1 tm_semi_bold tm_primary_color tm_accent_bg_10">Full Name</th>
                                        <th class="tm_width_1 tm_semi_bold tm_primary_color tm_accent_bg_10 tm_text_right">Email</th>
                                        <th class="tm_width_2 tm_semi_bold tm_primary_color tm_accent_bg_10 tm_text_right">Phone</th>
                                        <th class="tm_width_2 tm_semi_bold tm_primary_color tm_accent_bg_10 tm_text_right">Address</th>
                                        <th class="tm_width_2 tm_semi_bold tm_primary_color tm_accent_bg_10 tm_text_right">Price</th>
                                        <th class="tm_width_2 tm_semi_bold tm_primary_color tm_accent_bg_10 tm_text_right">Note</th>
                                        <th class="tm_width_2 tm_semi_bold tm_primary_color tm_accent_bg_10 tm_text_right">CreatedAt</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td class="tm_width_2 tm_text_right">@item.FullName</td>
                                            <td class="tm_width_1 tm_text_right">@item.EmailAddress</td>
                                            <td class="tm_width_2 tm_text_right">@item.ContactNumber</td>
                                            <td class="tm_width_2 tm_text_right">@item.Address</td>
                                            <td class="tm_width_2 tm_text_right">@item.Price</td>
                                            <td class="tm_width_2 tm_text_right">@item.Note</td>
                                            <td class="tm_width_2 tm_text_right">@item.CreatedAt</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                        <div class="tm_padd_15_20">
                            <p class="tm_mb2"><b class="tm_primary_color">Terms & Conditions:</b></p>
                            <ul class="tm_m0 tm_note_list">
                                <li>All claims relating to quantity or shipping errors.</li>
                                <li>Delivery dates are not guaranteed and Seller.</li>
                            </ul>
                        </div><!-- .tm_note -->
                    </div>
                </div>
            </div>
        </div>
</body>
</html>
